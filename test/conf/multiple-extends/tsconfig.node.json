{
  "compilerOptions": {
    "alwaysStrict": true,
    "declaration": true,
    "downlevelIteration": true,
    "emitBOM": false,
    "emitDeclarationOnly": true,
    "emitDecoratorMetadata": true,
    "lib": [
      "ES2017",
      "ES2018",
      "ES2019",
      "ES2020",
      "ES2021",
      "ES2022",
    ],
    "module": "NodeNext",
    "moduleResolution": "node",
    "target": "ES2022",
  },
  "exclude": [
    "lib",
  ],
  "include": [],
}
