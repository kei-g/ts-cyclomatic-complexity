  "devDependencies": {
    "@actions/core": "^1.11.1",
    "@types/eslint": "^9.6.1",
    "@types/mocha": "^10.0.10",
    "@types/node": "^24.7.2",
    "esbuild": "^0.25.10",
    "esbuild-register": "^3.6.0",
    "eslint": "^9.37.0",
    "globals": "^16.4.0",
    "mocha": "^11.7.4",
    "npm-run-all": "^4.1.5",
    "nyc": "^17.1.0",
    "rimraf": "^6.0.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.1"
